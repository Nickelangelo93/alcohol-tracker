{
  "expo": {
    "name": "Alcohol Tracker",
    "slug": "alcohol-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "alcohol-tracker",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBF6F0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alcoholtracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0f"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alcoholtracker.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "name": "Alcohol Tracker",
      "shortName": "DrinkTracker",
      "description": "Track your alcohol consumption mindfully",
      "themeColor": "#FBF6F0",
      "backgroundColor": "#FBF6F0"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png"
        }
      ]
    ]
  }
}
